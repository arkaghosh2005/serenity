{
  "name": "serenity",
  "version": "1.0.0",
   "scripts": {
    "install:all": "npm install --prefix client && npm install --prefix server && npm install",
    "client": "cd client && npm run dev",
    "server": "cd server && nodemon server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.1.0"
